<div class="wrapper">
    <!-- Navbar Start -->
    <%- include('../partials/header'); %>
        <%- include('../partials/Language.ejs'); %>

            <!-- Main Container Start -->
            <main class="main--container">
                <!-- Main Content Start -->
                <section class="main--content">
                    <div class="panel">
                        <!-- <div class="panel-heading">
                        <h3 class="panel-title">Search Results</h3>
                    </div> -->

                        <div class="panel-content">
                            <!-- Search Box Start -->
                            <div class="search--box">
                                <!-- <form action="#">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Type to search..." required>

                                    <div class="input-group-append">
                                        <button type="submit" class="btn btn-rounded btn-info"><i
                                                class="fa fa-search mr-1"></i> Search</button>
                                    </div>
                                </div>
                            </form> -->

                                <ul class="options">
                                    <li class="active">
                                        <a href="#">
                                            <%= i18n.__("Message")%>
                                        </a>
                                    </li>
                                    <!-- <li><a href="#">Users</a></li> -->
                                    <!-- <li><a href="#">Messages</a></li> -->
                                </ul>
                            </div>
                            <!-- Search Box End -->

                            <!-- Search Results Start -->
                            <div class="search--results">
                                <p class="stats">
                                    <%= totalNumItems %>
                                        <%= i18n.__("Results Found")%>
                                </p>

                                <ul class="results list-unstyled">
                                    <% tickets.forEach(function(ticket) { %>
                                        <li>
                                            <div class="link" style="color: rgb(139, 4, 4)">
                                                <%= moment(ticket.createdAt).fromNow() %>
                                            </div>
                                            <a href="#">
                                                <div class="link">
                                                    <div class="link">
                                                        name :
                                                        <%= ticket.name %>
                                                    </div>
                                                    <div class="link">
                                                        Date Of Birth :
                                                        <%= ticket.DateOfBirth %>
                                                    </div>
                                                    <div class="link">
                                                        flying :
                                                        <%= ticket.country %>
                                                    </div>
                                                    <div class="link">
                                                        departure Date :
                                                        <%= ticket.departureDate %>
                                                    </div>
                                                    <div class="link">
                                                        Return Date :
                                                        <%= ticket.returnDate %>
                                                    </div>
                                                    <div class="link">
                                                        flying Type : Return
                                                    </div>
                                                    <%= i18n.__("phone")%>
                                                        :
                                                        <%= ticket.phone %>
                                                            </span>
                                                            <div class="link">
                                                                <%= i18n.__("Email")%>
                                                                    :
                                                                    <%= ticket.email %>
                                                            </div>
                                                            <div class="link">
                                                                child :
                                                                <%= ticket.child %>
                                                            </div>
                                                            <div class="link">
                                                                adult :
                                                                <%= ticket.adult %>
                                                            </div>
                                                            <div class="link">
                                                                Infants :
                                                                <%= ticket.infants %>
                                                            </div>
                                            </a>
                                        </li>
                                        <% }); %>
                                </ul>

                                <%- include('../../partials/pagination'); %>
                                    </div>
                                    <!-- Search Results End -->
                            </div>
                        </div>
                </section>
                <!-- Main Content End -->

                <!-- Main Footer Start -->
                <footer class="main--footer main--footer-light">
                    <p>
                        <%= i18n.__("Copyright  Sartak car companyÂ©2022")%>.
                    </p>
                </footer>
