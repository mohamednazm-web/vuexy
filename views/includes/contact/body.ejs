    <section class="contact-area section--padding">
        <div class="container">
            <div class="row">
                <div class="col-lg-8">
                    <div class="form-box">
                        <div class="form-title-wrap">
                            <h3 class="title"><%= i18n.__("GET IN TOUCH")%></h3>
                            
                            <p class="font-size-15">If you have any questions, do not hesitate to contact us</p>
                        </div>
                        <!-- form-title-wrap -->
                        <div class="form-content ">
                            <div class="contact-form-action">
                                <form class="row messenger-box-form" action="/admin_dashboard/getContact" method="post">
                                    <div class="alert alert-success messenger-box-contact__msg col-lg-12" style="display: none" role="alert">
                                        Thank You! Your message has been sent.
                                    </div>
                                    <div class="col-lg-6 responsive-column">
                                        <div class="input-box messenger-box-input-wrap">
                                            <label class="label-text" for="name"><%= i18n.__("Name")%></label>
                                            <div class="form-group">
                                                <span class="la la-user form-icon"></span>
                                                <input class="form-control" type="text" id="name" name="name" placeholder=<%= i18n.__("Name")%> required>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end col-lg-6 -->
                                    <div class="col-lg-6 responsive-column">
                                        <div class="input-box messenger-box-input-wrap">
                                            <label class="label-text" for="email"><%= i18n.__("Email")%></label>
                                            <div class="form-group">
                                                <span class="la la-envelope-o form-icon"></span>
                                                <input class="form-control" type="email" name="email" id="email" placeholder=<%= i18n.__("Email")%> required>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end col-lg-6 -->
                                    <div class="col-lg-12">
                                        <div class="input-box messenger-box-input-wrap">
                                            <label class="label-text" for="message"><%= i18n.__("Name")%></label>
                                            <div class="form-group">
                                                <span class="la la-pencil form-icon"></span>
                                                <textarea class="message-control form-control" name="message" id="message" placeholder=<%= i18n.__("Write")%> required></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end col-lg-12 -->
                                    <div class="col-lg-12">
                                        <div class="btn-box messenger-box-input-wrap">
                                            <button name="submit" type="submit" class="theme-btn send-message-btn" id="send-message-btn"><%= i18n.__("SEND A MESSAGE")%></button>
                                        </div>
                                    </div>
                                    <!-- end col-lg-12 -->
                                </form>
                            </div>
                            <!-- end contact-form-action -->
                        </div>
                        <!-- end form-content -->
                    </div>
                    <!-- end form-box -->
                </div>
                <!-- end col-lg-8 -->
                <div class="col-lg-4">
                    <div class="form-box">
                        <div class="form-title-wrap">
                            <h3 class="title"><%= i18n.__("contact")%></h3>
                        </div>
                        <!-- form-title-wrap -->
                        <div class="form-content">
                            <div class="address-book">
                                <ul class="list-items contact-address">

                                    <li>
                                        <i class="la la-phone icon-element"></i>
                                        <h5 class="title font-size-16 pb-1"><%= i18n.__("phone")%></h5>
                                        <p class="map__desc">Telephone:+00447733774242</p>
                                        <p class="map__desc">Telephone:+96407733774242</p>
                                    </li>
                                    <li>
                                        <i class="la la-envelope-o icon-element"></i>
                                        <h5 class="title font-size-16 pb-1"><%= i18n.__("Email")%></h5>
                                        <p class="map__desc">info@sartaktravel.com</p>
                                    </li>
                                </ul>
                                <ul class="social-profile text-center">
                                    <li><a href="https://m.facebook.com/pages/category/Travel-Agency/SARTAKTRAVEL1/photos/" target="_blank"><i class="lab la-facebook-f"></i></a></li>
                                    <li><a href="https://wa.me/+447568686262" target="_blank"><i class="lab la-whatsapp"></i></a></li>
                                    <li><a href="#"><i class="lab la-instagram"></i></a></li>


                                    <!--  <li><a href="#"><i class="lab la-twitter"></i></a></li>
                                    <li><a href="#"><i class="lab la-instagram"></i></a></li>
                                    <li><a href="#"><i class="lab la-linkedin-in"></i></a></li>
                                    <li><a href="#"><i class="lab la-youtube"></i></a></li> -->
                                </ul>
                            </div>
                        </div>
                        <!-- end form-content -->
                    </div>
                    <!-- end form-box -->
                </div>
                <!-- end col-lg-4 -->
            </div>
            <!-- end row -->
        </div>
        <!-- end container -->
    </section>
    <!-- end contact-area -->
    <!-- ================================
END CONTACT AREA
