<%- include('../partials/header'); %>
    <div class="page-header-wrap">
        <img src="assets/img/bg/bannerimg.jpg" class="mg-fluid img-bg">
    </div>
    <!--== End Page Header Area ==-->

    <!--== Start Page Content Wrapper ==-->
    <% if(locals.selectedI18n==='en' ) {%>
        <div class="page-content-wrapper sm-top pt-4">
            <% } else {%>
                <div class="page-content-wrapper sm-top" style="direction: rtl;">
                    <style>
                        .org {
                            text-align: right;
                        }
                    </style>
                    <% } %>
                        <div class="contact-page-content">
                            <div class="contact-info-wrapper">
                                <div class="container">
                                    <div class="row mtn-30 font_rudawregular2">
                                        <div class="col-sm-6 col-lg-4 ">
                                            <div class="contact-info-item">
                                                <div class="con-info-icon">
                                                    <i class="ion-ios-location-outline"></i>
                                                </div>
                                                <div class="con-info-txt pb-4">
                                                    <h4 class="oop">
                                                        <%= i18n.__("Our Location")%>
                                                    </h4>
                                                    <p>
                                                        <%= i18n.__("address")%>
                                                    </p>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-6 col-lg-4">
                                            <div class="contact-info-item">
                                                <div class="con-info-icon">
                                                    <i class="ion-iphone"></i>
                                                </div>
                                                <div class="con-info-txt">
                                                    <h4>
                                                        <%= i18n.__("Contact us Anytime")%>
                                                    </h4>

                                                    <p class=" pt-2">
                                                        <%= i18n.__("phone1")%> -
                                                            <%= i18n.__("phone2")%>
                                                                <p>
                                                                    <%= i18n.__("phone3")%> -
                                                                        <%= i18n.__("phone4")%>
                                                                </p>

                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-6 col-lg-4">
                                            <div class="contact-info-item">
                                                <div class="con-info-icon">
                                                    <i class="ion-ios-email-outline"></i>
                                                </div>
                                                <div class="con-info-txt">
                                                    <h4>
                                                        <%= i18n.__("contact via email")%>
                                                    </h4>
                                                    <p class=" pb-2">Hershcars86@Gmail.com

                                                    </p>
                                                    <br>
                                                    <p></p>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="contact-form-wrapper sm-top pb-2">
                                <div class="container">
                                    <div class="row">
                                        <div class="col-12">
                                            <div class="contact-form-content">
                                                
                                                        <h2 class="org">
                                                            <%= i18n.__("GET IN TOUCH")%>
                                                        </h2>

                                                        <div class="contact-form-wrap">
                                                            <form action="/admin_dashboard/getContact" method="post"
                                                                id="contact-form">
                                                                <div class="contact-form-inner">
                                                                    <div class="row">
                                                                        <div class="col-md-6">
                                                                            <div class="input-item">
                                                                                <label class="sr-only" for="name">
                                                                                    <%= i18n.__("Name")%>
                                                                                </label>
                                                                                <input type="text" name="name" id="name"
                                                                                    placeholder=<%=i18n.__( "Name" )%>
                                                                                required />
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-md-6">
                                                                            <div class="input-item">
                                                                                <label class="sr-only" for="email">
                                                                                    <%= i18n.__("Email")%>
                                                                                </label>
                                                                                <input type="email" name="email"
                                                                                    id="email"
                                                                                    placeholder=<%=i18n.__( "Email" )%>
                                                                                required />
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-md-6">
                                                                            <div class="input-item">
                                                                                <label class="sr-only" for="Phone">
                                                                                    <%= i18n.__("Phone")%>
                                                                                </label>
                                                                                <input type="tell" name="phone"
                                                                                    id="phone"
                                                                                    placeholder=<%=i18n.__( "Phone" )%>
                                                                                required />
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-12">
                                                                            <div class="input-item">
                                                                                <label class="sr-only" for="subject">
                                                                                    <%= i18n.__("subject")%>
                                                                                </label>
                                                                                <input type="text" name="subject"
                                                                                    id="subject"
                                                                                    placeholder=<%=i18n.__( "subject"
                                                                                    )%> required />
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-12">
                                                                            <div class="input-item">
                                                                                <label class="sr-only" for="message">
                                                                                    <%= i18n.__("Write Message")%>
                                                                                </label>
                                                                                <textarea name="message" id="message"
                                                                                    cols="30" rows="8"
                                                                                    placeholder=<%=i18n.__( "Write" )%>
                                                            required></textarea>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-12">
                                                                            <div class="input-item">
                                                                                <button type="submit"
                                                                                    style="color:white;"
                                                                                    class="btn btn-brand">
                                                                                    <%= i18n.__("SEND A MESSAGE")%>
                                                                                </button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </form>
                                                        </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="contact-map-wrapper">
                                <iframe
                                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3221.2508094114737!2d43.99850141490898!3d36.16045091114876!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4007239ea29c27c3%3A0x51bcd9b64d3ba73!2sHersh%20cars!5e0!3m2!1sen!2siq!4v1625487813730!5m2!1sen!2siq"
                                    width="100%" height="360" allowfullscreen="" loading="lazy"></iframe>
                                <!--             <div id="map_content" class="h-100" data-lat="36.160645808528116" data-lng="44.00064718199581" data-zoom="7"></div>
 -->
                            </div>
                        </div>
                </div>
                <!--== End Page Content Wrapper ==-->


                <%- include('../partials/footer'); %>