<div class="call-to-action-area">
    <div class="call-to-action-content-area bg-img" data-bg="assets/img/bg/bg-1.jpg">
        <div class="container">
            <div class="row">
                <div class="col-12 text-center">
                    <div class="call-to-action-txt">
                        <h2 class="font-change">
                            <%= i18n.__("All kind1")%>
                                <br>
                                <%= i18n.__("All kind2")%>

                        </h2>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="call-to-action-image-area">
        <div class="container">
            <div class="row">
                <div class="col-12 text-center">
                    <img src="/<%= bannerBottoms[0].image %>" alt="Car" />
                </div>
            </div>
        </div>
    </div>
</div>

<!--== Start Promotion Code Banner Area ==-->
<div class="promotion-code-area-wrapper sm-top">
    <div class="container container-wide">
        <div class="row">
            <div class="col-md-6">
                <div class="promotion-code-banner-item mb-sm-30">
                    <img src="/<%= bannerBottoms[1].image %>" alt="Banner" />
                </div>
            </div>

            <div class="col-md-6">
                <div class="promotion-code-banner-item">
                    <img src="/<%= bannerBottoms[2].image %>" alt="Banner" />
                </div>
            </div>
        </div>
    </div>
</div>
<!--== End Promotion Code Banner Area ==-->